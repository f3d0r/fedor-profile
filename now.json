{
    "alias": ["profile.fedor.me"],
    "builds": [{
            "src": "*.html",
            "use": "@now/html-minifier"
        },
        {
            "src": "assets/css/*.css",
            "use": "@now/static"
        },
        {
            "src": "assets/images/*.png",
            "use": "@now/static"
        },
        {
            "src": "assets/js/*.js",
            "use": "@now/static"
        }
    ],
    "github": {
        "autoAlias": true,
        "autoJobCancelation": true,
        "enabled": true,
        "silent": true
    },
    "name": "fedor-profile",
    "regions": ["all"],
    "version": 2
}